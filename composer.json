{
  "name": "gm314/siesta",
  "description": "PHP 7 ORM Library",
  "require": {
    "php": ">=8.1.0",
    "symfony/console": "*",
    "psr/log": "*",
    "gm314/nitria": "0.2.1",
    "gm314/common": "*",
    "ext-dom" : "*",
    "ext-libxml" : "*",
    "ext-json" : "*",
    "ext-mysqli" : "*"
  },
  "require-dev": {
    "codeception/codeception": "*",
    "phpdocumentor/phpdocumentor": "*"
  },
  "authors": [
    {
      "name": "Gregor MÃ¼ller",
      "email": "gm@gm314.de",
      "role": "Developer"
    }
  ],
